<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form">
	<Dialog title="데이터 추가">
		<content>
			<f:SimpleForm id="dialogForm">
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button text="Save" id="idSaveButton" type="Accept" press="onSubmitDialogButton" />
		</beginButton>
		<endButton>
			<Button text="Close" id="idCloseButton" type="Reject" press="onCloseDialogButton" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>